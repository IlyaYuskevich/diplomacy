{
  "lock": false,
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "esm:add": "deno run -A https://esm.sh/v127 add",
    "esm:update": "deno run -A https://esm.sh/v127 update",
    "esm:remove": "deno run -A https://esm.sh/v127 remove"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.2.0/",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.1.0",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/",
    "@svgdotjs/svg.js": "https://esm.sh/v127/@svgdotjs/svg.js@3.2.0",
    "color": "https://esm.sh/v127/*color@4.2.3"
  },
  "scopes": {
    "https://esm.sh/v127/": {
      "color-convert": "https://esm.sh/v127/color-convert@2.0.1",
      "color-string": "https://esm.sh/v127/color-string@1.9.1"
    }
  }
}